import java.util.*;
public class Main
{
    public int add(int a,int b,int c){
        if(a>b &&a>c){
            return a;
        }
        else if(b>a && b>c){
            return b;
        }
        else{
            return c;
        }
       
    }
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int b=sc.nextInt();
	    int c=sc.nextInt();
Main obj=new Main();
		System.out.println(obj.Max(a,b,c));
	}
}
