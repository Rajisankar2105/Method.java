import java.util.*;
public class Main
{
public int fact(int n){
    if(n==0 ||n==1){
        return 1;
    }else{
        return n*fact(n-1);
    }
}
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
Main obj=new Main();
int n=sc.nextInt();
int a[][]=new int[n][n];
System.out.println(obj.fact(n));
    }
}
