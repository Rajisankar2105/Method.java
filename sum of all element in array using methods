import java.util.*;
public class Main
{
    public int arr_add(int a[]){
        int sum=0;
        for(int i:a){
            sum+=i;
        }
        return sum;
    }

	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
Main obj=new Main();
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
    a[i]=sc.nextInt();
}
int res=obj.arr_add(a);
System.out.println(res);
	}
}
